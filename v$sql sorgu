SELECT module,
       parsing_schema_name,
       inst_id,
       sql_id,
       CHILD_NUMBER,
       sql_plan_baseline,
       sql_profile,
       plan_hash_value,
       sql_fulltext,
       TO_CHAR (last_active_time, 'DD/MM/YY HH24:MI:SS'),OPTIMIZER_COST,
       executions,elapsed_time,INST_ID,
       elapsed_time / executions / 1000 / 1000,
       rows_processed,
       sql_plan_baseline
  FROM gv$sql where last_active_time>sysdate-1/24 and sql_id='c77kj9q3qz6tk'
and executions <> 0 order by elapsed_time/executions desc
